local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("Raisen Premium", "Default")

local lockRageTab = DrRayLibrary.newTab("Lock/Rage", "ImageIdHere")

lockRageTab.newButton("Rework v3", "Raisen Premium Lock&Anti Gui", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/raisenscripts/Premiumv2/main/Rework-loader"))()
end)

lockRageTab.newButton("Aura X", "aura.cc", function()
    loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/8ed6eed3a56b7b56d8c96bddc8773f16.lua"))()
end)

lockRageTab.newButton("Tp walk v4", "Recommended for silent aim.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRealXORA/Roblox-Scripts/main/Tpwalk%20V4%20%5B%20BETA%20%5D.lua", true))()
end)

local otherGuisTab = DrRayLibrary.newTab("Other guis", "ImageIdHere")

otherGuisTab.newButton("Swagmode", "Swagmode v4", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/lerkermer/lua-projects/master/SwagModeV002"))()
end)

local miscTab = DrRayLibrary.newTab("Misc", "ImageIdHere")

miscTab.newButton("Zombie Animation", "get *Fe* zombie animation", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/raisenscripts/Premiumv2/main/Zombie%20animation"))()
end)


-- Script exucute edildiğinde karakter otomatik olarak bir mesaj yazar

-- Servisleri al
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Chat mesajını gönder
local function sendChatMessage(message)
    local chatRemote = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
    chatRemote:FireServer(message, "All")
end

-- Mesajı belirle ve gönder
local message = "Raisen Loaded!"  -- Bu mesajı istediğiniz gibi değiştirebilirsiniz
sendChatMessage(message)


loadstring(game:HttpGet("https://raw.githubusercontent.com/raisenscripts/Premiumv2/main/commands"))()
loadstring(game:HttpGet("https://raw.githubusercontent.com/raisenscripts/Premiumv2/main/Emoji"))()
